{% use '_blocks.html.twig' %} 

<!DOCTYPE HTML>

<html>

	<head>
		
		<script src="https://cdn.tailwindcss.com"></script>  
		<link rel="stylesheet" type="text/css" href="css/main.css" />
		<link rel="stylesheet" type="text/css" href="css/custom.css" />
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">

		{{ block('head_brand') }}
	</head>

	<body class="bg-gray-300">
		{{ block('header') }}
		{{ block('menu_overlay') }}

		
		
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
		<script src="js/modernizr.custom.js"></script>
		<script src="js/scrollReveal.min.js"></script>
		<script src="js/handlebars-v4.0.4.js"></script>
		<script src="js/main.js"></script>
		<script id="intro-template" type="text/x-handlebars-template">
		{% verbatim %}
		  	<div class="intro-{{ introPosition }}">
				{{#if introTitleBig}} <h1 class="big">{{ introTitleBig }}</h1> {{/if}}
				{{#if introTitle}} <h1>{{ introTitle }}</h1> {{/if}}
				{{#if introDate}} <h3>{{ introDate }}</h3> {{/if}}
				{{#if introHr}} <hr> {{/if}}
				{{#if introChapeau}} <p class="chapeau">{{ introChapeau }}</p>	{{/if}}
				{{#if introChapeauBig}} <p class="chapeau big">{{ introChapeauBig }}</p> {{/if}}
			</div>
		{% endverbatim %}
		</script>

		<script id="entry-template" type="text/x-handlebars-template">
		{% verbatim %}
			{{#each entries}}
		  	<li>
				{{#if entryYear}}
				<time class="cbp_tmtime" datetime="2013-04-10 18:30">
				<!-- <span>4/10/13</span> --> <span>{{#if entryDay}} {{ entryDay }} <br>{{/if}}{{#if entryMonth}} {{ entryMonth }} <br>{{/if}}{{ entryYear }}&nbsp</span>
				</time>{{/if}}
				<div class="cbp_tmicon"><i class="fa fa-{{ entryIcon }}"></i></div>
				<div class="cbp_tmlabel" {{#if entryEffect}} data-sr='{{ entryEffect }}'{{/if}}>
					<h2>{{{ entryTitle }}}</h2>
					{{#if entryVideo}} <div class="video-container"><iframe height="auto" src="{{ entryVideo }}?&theme=dark&autohide=2&cc_load_policy=1&showinfo=0"frameborder="0" frameborder="0" allowfullscreen></iframe></div> {{/if}}
					{{#if entryImage}} <img src="img/{{ entryImage }}" alt=""> {{/if}}
					{{#if entryImageLegend}} <p class="legende py-2 text-xs lead-normal text-left ">{{{ entryImageLegend }}} {{/if}}</p>
					{{#if entryAudio}}
						<div class="w-full py-12 px-16 bg-custom-audio rounded-xl mb-8">
							<audio controls class="mx-auto w-full">
								<source src="audio/{{entryAudio}}" type="audio/mpeg">
							  Your browser does not support the audio element.
							  </audio> 
						</div>
					{{/if}}
						{{#if entryInsta}} {{{entryInsta}}} {{/if}}
						{{#if entryText}}<p>{{{ entryText }}}</p>{{/if}}
					{{#if entryLinkText}}
						<br>
						<a href="{{ entryLink }}" target="_blank" class="text-black font-bold underline decoration-2"><i class="fa fa-arrow-right mr-2"></i> {{ entryLinkText }}</a>
					{{/if}}
				</div>
			</li>
			{{/each}}
		{% endverbatim %}
		</script>

		

		

		<div class="hero mt-12">
			<div class="container">
				<h1>Lorem ipsum dolor sit amet consectetur adipisicing elit. Aperiam sapiente repudiandae accusantium veniam beatae assumenda maxime voluptate quas. Eius placeat soluta distinctio ad omnis corporis id blanditiis. Animi, quae maxime.</h1>
			</div>
			
		</div>
	
		<!-- <div class="clearfix">
			<div class="gradient"></div>
			<div class="gradient2"></div>
			<img src="img/header.jpg" alt="">
			<div class="intro-height">
				<div class="intro-container">
					
				</div>
			</div>
		</div> -->

		<div class="container">
			<!-- <header class="clearfix">
				<span>Blueprint</span>
				<h1>Vertical Timeline</h1>
				<nav>
					<a href="http://tympanus.net/Blueprints/ScrollingLayout/" class="icon-arrow-left" data-info="previous Blueprint">Previous Blueprint</a>
					<a href="http://tympanus.net/codrops/?p=14941" class="icon-drop" data-info="back to the Codrops article">back to the Codrops article</a>
				</nav>
			</header>	 -->
			<div class="main">
				<ul class="cbp_tmtimeline"></ul>
				<div class="credits">
				</div>
			</div>
		</div>

		<footer class="container py-8 max-w-full relative">   
			<div class="w-80 top-1/2 left-1/2 -translate-x-2/4 -translate-y-2/4 absolute">
				{{ block('credits') }}  
			</div>
		</footer> 

		

		<script src="js/header.js"></script>

	</body>
</html>
